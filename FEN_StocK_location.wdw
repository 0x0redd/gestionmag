    name : BTN_Valider
    identifier : 0x28194e9927508bb5
    internal_properties : 
    properties :
     x : 721
     y : 498
     width : 109
     height : 32
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1+
         // Version 1
         // Description 
         // Bouton en validation
         
        type : 14
      -
        code : |1-
         // 1. Récupérer les valeurs saisies ou sélectionnées
         sNomArticle	est une chaîne	= COMBO_Article..Libellé
         sNomStock	est une chaîne	= COMBO_Stock..Libellé
         nQuantite	est un entier	= Val(Quantitée)
         sDate		is string		= DateVersChaîne(DateSys(), "AAAA-MM-JJ HH:MM:SS")
         
         // 2. Exécuter la requête pour récupérer l'ID de l'article
         
         
         REQ_ID_PAR_NOM.ParamNom = sNomArticle
         SI REQ_ID_PAR_NOM.ParamNom <> sNomArticle ALORS
         	REQ_ID_PAR_NOM.ParamNom = sNomArticle
         	HExécuteRequête(REQ_ID_PAR_NOM)
         FIN
         nItemID est un entier = REQ_ID_PAR_NOM.id
         
         // 3. Exécuter la requête pour récupérer l'ID du stock
         REQ_select_stocklocation.location_name = sNomStock
         SI HExécuteRequête(REQ_select_stocklocation) = Faux OU PAS HLitPremier(REQ_select_stocklocation) ALORS
         	Erreur("Stock non trouvé.")
         	RETOUR
         FIN
         nLocationID est un entier = REQ_select_stocklocation.id
         
         // 4. Exécuter la requête d’insertion
         REQ_Ajout_ItemQuantities.pItem_id		= nItemID
         REQ_Ajout_ItemQuantities.pLocation_id	= nLocationID
         REQ_Ajout_ItemQuantities.pQuantity		= nQuantite
         REQ_Ajout_ItemQuantities.pCreated_at			= sDate
         
         SI HExécuteRequête(REQ_Ajout_ItemQuantities) = Faux ALORS
         	Erreur("Échec de l'ajout : " + HErreurInfo())
         	RETOUR
         FIN
         
         Info("Quantité insérée avec succès dans item_quantities.")
         
         Total_paiement = total_achat_cal
        type : 18
