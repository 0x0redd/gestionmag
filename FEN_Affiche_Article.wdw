 code_elements :
  type_code : 1
  p_codes :
   -
     code : |1+
      PROCÉDURE MaFenêtre()
      // Nom de l'article à afficher
      sNomArticle est une chaîne = "papier"
      
      // Affectation du paramètre
      REQ_test_select_article.ParamName = sNomArticle
      
      // Exécution de la requête
      SI PAS HExécuteRequête(REQ_test_select_article) ALORS
      	Erreur("Impossible de charger l'article : " + HErreurInfo())
      	RETOUR
      FIN
      
      // Lecture du premier enregistrement
      HLitPremier(REQ_test_select_article)
      SI HEnDehors() ALORS
      	Erreur("Aucun article trouvé avec le nom '" + sNomArticle + "'")
      	RETOUR
      FIN
      
      // Remplissage des champs
      EDT_NomArticle		= REQ_test_select_article.name
      COMBO_Categorie		= REQ_test_select_article.category
      EDT_PrixAchat		= NumériqueVersChaîne(REQ_test_select_article.cost_price, "D")
      EDT_PrixVente		= NumériqueVersChaîne(REQ_test_select_article.unit_price, "D")
      EDT_Description		= REQ_test_select_article.description
      
      // Affichage de l’image si elle existe
      SI REQ_test_select_article.pictures <> Null ET Taille(REQ_test_select_article.pictures) > 0 ALORS
      	sCheminTemp est une chaîne = fRepTemp() + "\image_temp.jpg"
      	SI fSauveBuffer(sCheminTemp, REQ_test_select_article.pictures) ALORS
      		IMG_Photo = sCheminTemp
      	SINON
      		IMG_Photo = ""
      	FIN
      SINON
      	IMG_Photo = ""
      FIN
