       name : SC_Recherche1
       identifier : 0x282f408f12e6ef42
       internal_properties : 
       properties :
        x : 348
        y : 10
        width : 489
        height : 44
        anchor : 8
        tab_order : 1
       style : {}
       controls :
        -
          name : SAI_Recherche
          identifier : 0x282f408f12e7ef42
          internal_properties : 
          properties :
           width : 286
           height : 47
           accelerator_key : 65535
           tab_order : 0
          style : {}
          type : 2
          code_elements :
           type_code : 3
           p_codes :
            -
              code : |1+
               // Largeur la plus petite définie à la largeur nécessaire pour voir le texte d'indication, centrée entre les retraits
               SAI_Recherche..Titrenote = SAI_Recherche..RetraitGauche +  TexteLargeur(SAI_Recherche,SAI_Recherche..indication,tMonoligne) + SAI_Recherche..RetraitGauche
               
               // Applique la largeur initiale
               SAI_Recherche..largeur = SAI_Recherche..TitreNote
               
               //Procédure d'adaptation de la largeur du champ
               procédure interne MajLargeur()
               	// Adapte la largeur de la zone de saisie à la frappe, sans dépasser le superchamp
               	soit nLargeurTexte = TexteLargeur(SAI_Recherche,SAI_Recherche,tMonoligne)
               	// Centre entre les retraits
               	soit nTailleMinPourTexte = SAI_Recherche..RetraitGauche + nLargeurTexte + SAI_Recherche..RetraitGauche
               	SAI_Recherche..Largeur = Min(Max(SAI_Recherche..LargeurInitiale,nTailleMinPourTexte),SC_Recherche1..Largeur)
               fin
               
               // Met à jour la largeur à chaque frappe
               MaFenêtre..Traitement[trtModification] += MajLargeur
               // Met à jour la largeur au redimensionnement de la fenêtre 
               mafenetre..Traitement[trtChangeTaille] += MajLargeur
              type : 14
            -
              code : |1-
               // Agrandit la zone de saisie si elle est vide
               SI MoiMême = "" ALORS	
               	AnimationJoueSurPropriétéChamp(MoiMême,<§R182f408f00b9ef62¤fr-FR¤Largeur¤en-GB¤Width¤es-ES¤Ancho¤0§>,MoiMême..Largeur,MoiMême..LargeurInitiale,150 ms)
               fin
              type : 15
            -
